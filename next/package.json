{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:* -c",
    "lint:eslint": "eslint --ext .js,.ts,.jsx,.tsx --ignore-path .lintignore . --max-warnings 0",
    "lint:prettier": "prettier --check  --ignore-path .lintignore .",
    "lint:tsc": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate:chakra": "chakra-cli tokens ./src/lib/chakra.ts"
  },
  "dependencies": {
    "@graphql-yoga/node": "^2.4.0",
    "graphql": "^16.4.0",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  }
}
